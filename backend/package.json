        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/uuid": "^9.0.8",
        "prettier": "^3.2.5"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@prisma/client": "^5.14.0",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "cors": "^2.8.5",
        "express": "^4.19.2",
        "jsonwebtoken": "^9.0.2",
        "prisma": "^5.14.0",
        "socket.io": "^4.7.5",
        "uuid": "^9.0.1"
